  "dependencies": {
    "@sentry/browser": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@typeform/embed": "^1.27.1",
    "bottega": "https://github.com/KSF-Media/bottega-javascript-client.git#7e765d472a404bd14579edc199619d5112936b57",
    "browser-sync": "^2.27.9",
    "cheerio": "1.0.0-rc.9",
    "create-react-class": "^15.7.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.43",
    "html-to-react": "^1.4.7",
    "persona": "https://github.com/KSF-Media/persona-javascript-client.git#812432688d5ca1b2688e5cc4fa986467c1106528",
    "querystring-es3": "^0.2.1",
    "react": "^18.1.0",
    "react-date-picker": "^8.0.3",
    "react-dom": "^18.1.0",
    "react-imported-component": "^6.4.1",
    "react-router-dom": "^6.3.0",
    "url": "^0.11.0",
    "uuid": "3.4.0",
    "uuid-validate": "^0.0.3",
    "xhr2": "^0.2.1"
  },
  "scripts": {
    "start": "node -r dotenv/config -e 'require(\"./output/Main/index\").main()' dotenv_config_path=.env.local",
    "start:test": "node -r dotenv/config -e 'require(\"./output/Main/index\").main()' dotenv_config_path=.env.test",
    "start-dev": "npm run build:watch",
    "build": "npm run build-app && npm run build-spa",
    "build:watch": "concurrently \"npm:build-app:watch\" \"npm:build-spa:watch\"",
    "build-app": "spago build",
    "build-app:watch": "spago build --watch",
    "build-spa": "node -r dotenv/config build.mjs dotenv_config_path=.env.local",
    "build-spa:watch": "node -r dotenv/config build.mjs dotenv_config_path=.env.local serve",
    "build-deploy-preview": "npm run build-spa",
    "pre-test": "cp .env.local .env.test && echo 'DISABLE_ADS=1' >> .env.test",
    "test": "npm run pre-test && start-server-and-test start:test http://localhost:8080 'spago -x test.dhall test'"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@chialab/esbuild-plugin-html": "^0.15.37",
    "concurrently": "^7.2.1",
    "esbuild-plugin-less": "^1.1.7",
    "puppeteer": "^14.1.1",
    "start-server-and-test": "^1.14.0",
    "tiny-glob": "^0.2.9"
  },
  "browserslist": [
    "> 0.25%",
    "not dead",
    "IE 11"
  ]
}
