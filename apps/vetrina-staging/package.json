{
  "name": "vetrina-staging",
  "version": "1.0.0",
  "description": "Testing platform of vetrina",
  "main": "index.js",
  "dependencies": {
    "@ksf-media/vetrina": "file:../../packages/vetrina",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "parcel index.html -p 8000",
    "build-vetrina": "yarn --cwd '../../packages/vetrina' build-npm",
    "build": "yarn build-vetrina && yarn clean && yarn install && parcel build index.html --no-cache --public-url ./",
    "clean": "rm -rf .cache dist node_modules"
  }
}
