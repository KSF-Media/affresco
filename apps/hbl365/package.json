{
  "name": "@affresco/hbl365",
  "version": "1.0.0",
  "description": "HBL 365 purchase",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "basscss": "^8.0.4",
    "create-react-class": "^15.6.3",
    "less": "^3.8.1",
    "react": ">=15.5",
    "react-dom": ">=15.5"
  },
  "scripts": {
    "start": "parcel index.html -p 8000",
    "build": "spago build && parcel build index.html --public-url ./",
    "wait-for-parcel": "perl -e '$a = 1; while ($a) { $a = system(\"curl http://localhost:8000/\"); sleep(1)}'",
    "test": "spago build && yarn install --cache-folder ../../.yarn-cache && yarn start & yarn wait-for-parcel && spago test"
  }
}
