{
  "name": "@affresco/prenumerera",
  "version": "1.0.0",
  "description": "Frontend for making subscriptions.",
  "license": "MIT",
  "dependencies": {
    "basscss": "^8.0.4",
    "create-react-class": "^15.6.3",
    "less": "^3.8.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "scripts": {
    "start": "parcel index.html -p 8000",
    "build-purs": "cd ../.. && spago bundle-module -m Prenumerera.Main -t apps/prenumerera/index.js --path \"packages/*/src/**/*.purs\" --path \"apps/*/src/**/*.purs\" ",
    "build": "yarn build-purs && parcel build index.html --no-cache && cp xd_receiver.html ./dist"
  }
}
