{
  "name": "@affresco/prenumerera",
  "version": "1.0.0",
  "description": "Prenumerera",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "basscss": "^8.0.4",
    "create-react-class": "^15.6.3",
    "less": "^3.8.1",
    "react": ">=15.5",
    "react-dom": ">=15.5"
  },
  "scripts": {
    "start": "parcel index.html -p 8000",
    "build": "spago build && parcel build index.html --no-cache --public-url ./ --no-source-maps",
    "wait-for-parcel": "perl -e '$a = 1; while ($a) { $a = system(\"curl http://localhost:8000/\"); sleep(1)}'",
    "test": "#spago build && npm install && npm run start & npm run wait-for-parcel && spago test"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "parcel-bundler": "^1.12.3"
  }
}
